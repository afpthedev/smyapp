<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Appointment.
    -->
    <changeSet id="20250919114556-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="appointment"
                                 constraintName="fk_appointment__created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"
                                 />

        <addForeignKeyConstraint baseColumnNames="type_id"
                                 baseTableName="appointment"
                                 constraintName="fk_appointment__type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="appointment_type"
                                 />

        <addForeignKeyConstraint baseColumnNames="appointment_id"
                                 baseTableName="rel_appointment__participants"
                                 constraintName="fk_rel_appointment__participants__appointment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="appointment"
                                 />

        <addForeignKeyConstraint baseColumnNames="participants_id"
                                 baseTableName="rel_appointment__participants"
                                 constraintName="fk_rel_appointment__participants__participants_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"
                                 />
    </changeSet>
</databaseChangeLog>
