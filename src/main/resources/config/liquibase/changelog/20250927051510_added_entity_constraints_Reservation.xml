<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
  <!--
      Added the constraints for entity Reservation.
  -->
  <changeSet id="20250927051510-2" author="jhipster">

    <addForeignKeyConstraint baseColumnNames="service_id"
                             baseTableName="reservation"
                             constraintName="fk_reservation__service_id"
                             referencedColumnNames="id"
                             referencedTableName="offered_service"
    />

    <addForeignKeyConstraint baseColumnNames="customer_id"
                             baseTableName="reservation"
                             constraintName="fk_reservation__customer_id"
                             referencedColumnNames="id"
                             referencedTableName="customer"
    />

    <addForeignKeyConstraint baseColumnNames="business_id"
                             baseTableName="reservation"
                             constraintName="fk_reservation__business_id"
                             referencedColumnNames="id"
                             referencedTableName="business"
    />
  </changeSet>
</databaseChangeLog>
